<h1>Welcome to TinyApp</h1>

<% if(!user) { %>
  <form action="/login" method="POST">
    <input type="text" name="username" placeholder="username">
    <button>Login</button>
  </form>
<% } else { %>
  <h3><%= user %></h3>
  <form action="/logout" method="POST">
   <button>Logout</button>
  </form>
<% } %>

